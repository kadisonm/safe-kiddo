{
  "manifest_version": 3,
  "name": "Safe Kiddo",
  "description": "A browser extension that provides parental controls to keep your kiddo safe online.",
  "options_page": "options.html",
  "background": {
    "scripts": ["background.bundle.js"]
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon-34.png"
  },
  "icons": {
    "128": "icon-128.png"
  }
}
